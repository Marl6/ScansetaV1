{
  "name": "scanseta-frontend",
  "version": "1.0.0",
  "description": "React frontend for Scanseta",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "start-db": "set DB_PORT=5002 && node Front_end/src/Database/server.js",
    "start-backend": "python Front_end/src/Backend/app.py",
    "start-react": "cd Front_end && set PORT=3002 && npm run start",
    "dev": "concurrently --kill-others-on-fail \"npm run start-db\" \"npm run start-backend\" \"npm run start-react\"",
    "dev-db-backend": "concurrently \"npm run start-db\" \"npm run start-backend\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@radix-ui/react-progress": "^1.1.3",
    "cors": "^2.8.5",
    "fuse.js": "^7.0.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "sqlite3": "^5.1.7"
  },
  "proxy": "http://localhost:5000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
